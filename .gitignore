# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/public_html/core/
/public_html/modules/contrib/
/public_html/themes/contrib/
/public_html/profiles/contrib/
/public_html/libraries/

# Ignore sensitive information
/public_html/sites/*/settings.php
# Make sure that we ignore all variations of settings.php file.
/public_html/sites/*/settings.*.php

# Ignore Drupal's file directory
/public_html/sites/*/files/
/public_html/files/

# Ignore the private files directory.
/drupal8-private/

# Ignore files generated by PhpStorm
/.idea/

# Ignore .env files as they are personal
/.env

# Ignore the DS_Store file generated by Finder.
**/.DS_Store
